<div class="container">
  <h2>Posts list</h2>

  {{#each posts}}
    <div class="card mb-3">
      <div class="card-body">
        <h2 class="card-title">{{title}}</h2>
        <p class="card-text">{{content}}</p>
        <p class="card-text">{{author}}</p>
        <a href="" class="btn btn-primary">More</a>
      </div>
    </div>
  {{/each}}
</div>